{
  "name": "sergio_wicramachine_tp2",
  "version": "1.0.0",
  "description": "Application de blog TP2",
  "main": "server.js",
  "scripts": {
    "start": "node --env-file=.env server.js",
    "dev": "nodemon --env-file=.env server.js",
    "make:migration": "npx sequelize-cli migration:generate --name",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "blog",
    "express",
    "sequelize",
    "jwt",
    "authentication"
  ],
  "author": "Wicramachine Sergio",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "nodemon": "^3.1.11",
    "sequelize-cli": "^6.6.5"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mysql2": "^3.16.0",
    "sequelize": "^6.37.7"
  }
}
